<?xml version="1.0"?>
<Obfuscator>
	<!-- ==== Paths ==== -->
	<Var name="InPath"  value="." />
	<Var name="OutPath" value="..\..\Obfuscated\2021" />
	
	<Var name="LogFile" value="$(OutPath)\mapping.txt" />
	<Var name="XmlMapping" value="true" />
	<Var name="RegenerateDebugInfo" value="true" />

	<!-- ==== Basic Strategy ==== -->
	<Var name="KeepPublicApi" value="true" />
	<Var name="HidePrivateApi" value="true" />
	<Var name="MarkedOnly" value="false" />

	<Var name="RenameProperties" value="true" />
	<Var name="RenameEvents" value="false" />
	<Var name="RenameFields" value="true" />

	<Var name="UseUnicodeNames" value="true" />
	<Var name="UseKoreanNames" value="false" />

	<Var name="HideStrings" value="true" />
	<Var name="OptimizeMethods" value="true" />
	<Var name="SuppressIldasm" value="true" />

	<Var name="ReuseNames" value="true" />
	<Var name="ReuseTypes" value="true" />

	<Var name="AnalyzeXaml" value="false" />
	<Var name="SkipGenerated" value="true" />

	<!-- ==== Where to resolve references ==== -->
	<AssemblySearchPath path="$(InPath)" />
	<AssemblySearchPath path="C:\Program Files\Autodesk\Revit 2021" />

	<!--==== Thêm skip nếu muốn ==== -->

	<!-- ==== Module cần obfuscate ==== -->
	<Module file="$(InPath)\$safeprojectname$.1.0.0.dll" />
</Obfuscator>